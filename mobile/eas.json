      "developmentClient": true,
      "distribution": "internal",
      "node": "20.18.0",
      "ios": {
        "simulator": true
      },
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://dev.tma.unca.info"
      }
    },
    "preview": {
      "distribution": "internal",
      "node": "20.18.0",
      "ios": {
        "simulator": false
      },
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://dev.tma.unca.info"
      }
    },
    "production": {
      "node": "20.18.0",
      "ios": {
        "simulator": false
      },
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://tma.unca.info"
      }
    }
  },
  "submit": {
    "production": {}
  }
