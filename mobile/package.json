{
    "name": "mobile",
    "version": "1.0.0",
    "main": "index.ts",
    "scripts": {
        "test": "jest",
        "test:verbose": "jest --verbose",
        "test:watch": "jest --watch",
        "lint:check": "eslint . --max-warnings 0",
        "lint:fix": "eslint . --fix",
        "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json}\"",
        "format:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json}\"",
        "type-check": "tsc --noEmit",
        "check": "npm run format:check && npm run lint:check",
        "fix": "npm run format:fix && npm run lint:fix",
        "update-ip": "./scripts/update-env-ip.sh",
        "start": "./scripts/update-env-ip.sh --quiet && expo start",
        "android": "./scripts/update-env-ip.sh --quiet && expo start --android",
        "ios": "./scripts/update-env-ip.sh --quiet && expo start --ios",
        "web": "expo start --web"
    },
    "dependencies": {
        "@expo/vector-icons": "^15.0.3",
        "@hookform/resolvers": "^5.2.2",
        "@tanstack/react-query": "^5.90.12",
        "axios": "^1.13.2",
        "expo": "~54.0.30",
        "expo-constants": "~18.0.12",
        "expo-file-system": "^19.0.21",
        "expo-font": "~14.0.10",
        "expo-linking": "~8.0.11",
        "expo-router": "^6.0.21",
        "expo-secure-store": "^15.0.8",
        "expo-sharing": "^14.0.8",
        "expo-status-bar": "~3.0.9",
        "react": "19.1.0",
        "react-dom": "19.1.0",
        "react-hook-form": "^7.69.0",
        "react-native": "0.81.5",
        "react-native-markdown-display": "^7.0.2",
        "react-native-paper": "^5.14.5",
        "react-native-safe-area-context": "~5.6.0",
        "react-native-screens": "~4.16.0",
        "react-native-web": "^0.21.2"
    },
    "devDependencies": {
        "@types/jest": "^30.0.0",
        "@types/react": "~19.1.0",
        "@typescript-eslint/eslint-plugin": "^8.52.0",
        "@typescript-eslint/parser": "^8.52.0",
        "eslint": "^8.57.1",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-unused-imports": "^4.3.0",
        "jest": "^29.7.0",
        "jest-expo": "^54.0.16",
        "prettier": "^3.7.4",
        "typescript": "~5.9.2"
